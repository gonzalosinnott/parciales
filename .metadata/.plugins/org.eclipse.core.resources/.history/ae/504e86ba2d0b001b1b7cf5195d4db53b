#ifndef CLIENTE_H_
#define CLIENTE_H_

#define TRUE 1
#define FALSE 0
#define LONG_NAME 50
#define LONG_CUIT 13 //1-11111111-1


typedef struct
{
	int cliente_id;
	int cliente_isEmpty;
	char cliente_name[LONG_NAME];
	char cliente_lastName[LONG_NAME];
	char cliente_Cuit[LONG_CUIT];
}Cliente;



#endif /* CLIENTE_H_ */
